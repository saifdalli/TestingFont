<script>
	async function postData(url = '', data = {}) {
	window.privacyBarToken='03959324-61da-4df9-b944-0b68f25754de';
	// console.log(" privacyBarToken Token => "+window.privacyBarToken);
	
	  const response = await fetch(url, {
		method: 'POST',
		mode: 'cors',
		cache: 'no-cache',
		credentials: 'same-origin', 
		headers: {
		  'Content-Type': 'application/json',
			'Authorization': 'Bearer ' + window.privacyBarToken,
		  // 'Content-Type': 'application/x-www-form-urlencoded',
		},
		redirect: 'follow',
		referrerPolicy: 'no-referrer', 
		body: JSON.stringify(data)
	  });
	  return response.json(); 
	}
	
	function UserAction(action) {
		window.current_ip = '10.64.250.72';
		// console.log(" window.current_ip => " + window.current_ip);
		
		window.REST_HOST='https://api.mobily.com.sa';
		console.log("REST_HOST >> >> "+window.REST_HOST);
		
		postData(window.REST_HOST + '/api/privacy-policy/v1/privacy-policy', 
		{ 
			ipAddress: window.current_ip,
			accept: action
		});
		  // .then(data => {
			// console.log(' => DATA : ' + data);
		  // });			
	}
</script>
<script>
	$( document ).ready(function() {
		
	  var privacyBar = localStorage.getItem("privacyBar");
	  if (privacyBar == null) {
		 // Second Time
		 $("#data_priv_bar").fadeIn("slow");
	  } else {
		 // First Time
		 $("#data_priv_bar").hide();
	  }
				  	
	$( "#data_priv_agree" ).click(function() {
		localStorage.setItem("privacyBar", "1");
		UserAction("yes");
		$("#data_priv_bar").fadeOut("slow");
		});
	$( "#data_priv_close" ).click(function() {
		UserAction("no");
		localStorage.setItem("privacyBar", "1");
		$("#data_priv_bar").fadeOut("slow")
		});
	});
</script>		
