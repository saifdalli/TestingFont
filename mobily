	<script>
	$(function () {
		$('a[href]').each(function () {
			
			var baseURL = "/wps/portal/web/";
			
			if ( $(this).attr("href").includes("/wps/portal/mobily40/") ){
				// console.log( "-- Found URL with invalid prefix: " + $(this).attr("href") );
				var hrefURL = $(this).attr("href")
				hrefURL = hrefURL.replace("/wps/portal/mobily40/", baseURL )				
				$(this).attr("href", hrefURL );
			}
		});
	});
